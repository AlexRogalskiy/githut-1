{
  "extends": [
    "standard",
    "eslint:recommended",
    "plugin:react/recommended",
    "prettier"
  ],
  "plugins": [
    "standard",
    "react",
    "promise"
  ],
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "env": {
    "node": true,
  },
  "rules": {
    "react/prop-types": [0 , {}],
    "no-duplicate-imports": off,
    "no-unused-vars": ['error', {
      argsIgnorePattern: '^_',
      varsIgnorePattern: '^_'
      } ],
  }
}
